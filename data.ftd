copy-regular: $heading-hero
copy-small: $heading-hero
fine-print: $heading-hero
blockquote: $heading-hero
source-code: $heading-hero
label-large: $heading-hero
label-small: $heading-hero
button-large: $heading-hero
button-medium: $heading-hero
button-small: $heading-hero
link: $heading-hero

-- ftd.text: Font
role: $manrope-demo-font

-- display-type:
types: $manrope-type

-- component display-type:
ftd.type-data types:

-- ftd.text: hello
role: $display-type.types.heading-hero

-- end: display-type
