-- import: fifthtry.github.io/manrope-font/assets as manrope-assets
-- import: fifthtry.github.io/manrope-font

-- ftd.type heading-hero-mobile:
line-height.px: 64
size.px: 48
letter-spacing.px: 0
weight: 700
font-family: $manrope-assets.fonts.Manrope


-- ftd.type heading-hero-desktop:
line-height.px: 104
size.px: 80
letter-spacing.px: 0
font-family: $manrope-assets.fonts.Manrope
weight: 700

-- ftd.responsive-type heading-hero:
;;desktop: $manrope-font.manrope-demo
desktop: $heading-hero-desktop
